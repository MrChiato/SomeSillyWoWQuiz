{
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/api/:path*"
        },
        {
            "source": "/(.*)",
            "has": [
                {
                    "type": "header",
                    "key": "accept",
                    "value": "text/html"
                }
            ],
            "destination": "/index.html"
        }
    ]
}